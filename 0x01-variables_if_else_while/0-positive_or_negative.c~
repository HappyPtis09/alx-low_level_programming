#include <stdlib.h>
#include <time.h>
#include <stdio.h>
/**
 * main - TASK 0 C
 * Positive anything is better than negative nothing
 * Return ; Always 0
 */
int main(void)
{
	int n;

	srand(time(0))
		n = rand() - RAND_MAX / 2;
	if (n > 0)
		printf("%d is positive", n);
	if else(n == 0)
			printf("%d is zero", n);
	else
		prinf("%d is negative", n);
	return (0);
}
